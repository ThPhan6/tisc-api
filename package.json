  "scripts": {
    "build": "ttsc",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\"",
    "compile": "ttsc && node ./dist/server.js",
    "create:database": "env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/create_database",
    "make:seed": "env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/make/seed",
    "make:migration": "env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/make/migration",
    "db:seed": "NODE_OPTIONS=--max_old_space_size=8192 env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/seed",
    "db:migrate": "NODE_OPTIONS=--max_old_space_size=8192 env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/migrate",
    "db:rollback": "env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/rollback",
    "db:reset": "env TS_NODE_PROJECT=\"console/tsconfig.json\" ts-node console/commands/reset",
    "test": "nyc env TS_NODE_PROJECT=\"tests/tsconfig.json\" mocha --reporter mochawesome --timeout 30000 -r ts-node/register \"**/tests/*.test.ts\""
  }

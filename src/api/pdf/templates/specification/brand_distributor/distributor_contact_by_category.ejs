<div class="general-specification-content">
    <table width="100%">
      <% data.forEach(function(item){ %>
        <tr>
            <td height="24" width="50%" class="normal-roboto-bold-8-18 bg-black" colspan="2">
              <span class="pdlr-8 color-white uppercase"><%= item.name %></span>
            </td>
        </tr>
        <% item.data.forEach(function(groupData){ %>
          <tr>
            <td class="primary-roboto-light-8-18 width-50-percent border-bottom-box-shadow">
              <span class="primary-roboto-bold-8-18"><%= groupData.distributor.location.name %></span><br/>
              <span><%= groupData.distributor.location.address %></span><br/>
              <% if (groupData.distributor.location.country_id == '-1') { %>
                <span>GLOBAL</span>
              <% } else { %>
                <span>
                  <% if (groupData.distributor.location.city_name) { %>
                    <%= groupData.distributor.location.city_name %>,
                  <% } %>
                  <%= groupData.distributor.location.state_name %>
                </span><br/>
                <span>
                  <% if (groupData.distributor.location.country_name) { %>
                    <%= groupData.distributor.location.country_name %>,
                  <% } %>
                  <%= groupData.distributor.location.postal_code %>
                </span><br/>
                <span><%= project.design_firm_official_website %></span>
              <% } %>

            </td>
            <td class="primary-roboto-light-8-18 width-50-percent border-bottom-box-shadow">
              <span class="primary-roboto-bold-8-18" style="opacity:0">XXXX</span><br/>
              <span>
                <span class="normal-garamond-light-8-18 uppercase mr-25">contact</span>
                <span class="primary-roboto-light-8-18"><%= groupData.distributor.location.first_name %> <%= groupData.distributor.location.last_name %></span>
              </span><br/>
              <span>
                <span class="normal-garamond-light-8-18 uppercase mr-13">work email</span>
                <span class="primary-roboto-light-8-18"><%= groupData.distributor.location.email %></span>
              </span><br/>
              <span>
                <span class="normal-garamond-light-8-18 uppercase mr-10">work phone</span>
                <span class="primary-roboto-light-8-18">+<%= groupData.distributor.location.phone_code %> <%= groupData.distributor.location.phone %></span>
              </span><br/>
              <span>
                <span class="normal-garamond-light-8-18 uppercase mr-8">work mobile</span>
                <span class="primary-roboto-light-8-18">+<%= groupData.distributor.location.phone_code %> <%= groupData.distributor.location.mobile %></span>
              </span><br/>
            </td>
          </tr>
        <% }); %>
      <% }); %>
    </table>
</div>
